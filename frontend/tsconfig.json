{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
, "src/components/layout/Navbar.tsx", "src/components/layout/Footer.jsx", "src/components/home/AboutPreview.jsx", "src/components/home/DownloadSection.jsx", "src/components/ui/ThreeDWaveCard.tsx", "src/app/about/startup-associate/page.tsx", "src/app/about/partners/page.jsx", "src/components/home/BoxSection.jsx", "src/components/ui/StatsCircle.tsx", "src/components/events/EditEventPage.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
